local lib = {}
function lib:CreateWindow(label)
	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local MainFrame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local UIPadding = Instance.new("UIPadding")
	local UIListLayout = Instance.new("UIListLayout")
	local Info = Instance.new("Frame")
	local TextLabel_2 = Instance.new("TextLabel")
	local TextLabel_3 = Instance.new("TextLabel")
	local UiCornerInfo = Instance.new("UICorner")
	local CloseButtonInfo = Instance.new("ImageButton")
	local Information = Instance.new("ImageButton")
	local TextLabel = Instance.new("TextLabel")
	local CloseButton = Instance.new("ImageButton")
	
	
	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0.121894643, 0, 0.368525892, 0)
	Frame.Size = UDim2.new(0, 208, 0, 19)
	Frame.ZIndex = 2

	MainFrame.Name = "MainFrame"
	MainFrame.Parent = Frame
	MainFrame.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
	MainFrame.Size = UDim2.new(0, 208, 0, 250)

	UICorner.CornerRadius = UDim.new(0, 4)
	UICorner.Parent = MainFrame

	UIPadding.Parent = MainFrame
	UIPadding.PaddingLeft = UDim.new(0, 27)
	UIPadding.PaddingTop = UDim.new(0, 40)

	UIListLayout.Parent = MainFrame
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0, 3)
	
	TextLabel.Parent = Frame
	TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.BackgroundTransparency = 1.000
	TextLabel.Position = UDim2.new(0, 0, 0.157894731, 0)
	TextLabel.Size = UDim2.new(0, 94, 0, 19)
	TextLabel.ZIndex = 2
	TextLabel.Font = Enum.Font.GothamSemibold
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000
	TextLabel.Text = label
	
	CloseButton.Name = "CloseButton"
	CloseButton.Parent = Frame
	CloseButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CloseButton.BackgroundTransparency = 1.000
	CloseButton.Position = UDim2.new(0.88516748, 0, 0, 0)
	CloseButton.Size = UDim2.new(0, 24, 0, 25)
	CloseButton.ZIndex = 2
	CloseButton.Image = "http://www.roblox.com/asset/?id=6031094678"
	CloseButton.MouseButton1Click:Connect(function()
		ScreenGui:Destroy()
	end)

	Info.Name = "Info"
	Info.Parent = Frame
	Info.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
	Info.Position = UDim2.new(1.0625, 0, 0, 0)
	Info.Size = UDim2.new(0, 149, 0, 56)
	Info.Visible = false

	TextLabel_2.Parent = Info
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.BackgroundTransparency = 1.000
	TextLabel_2.Position = UDim2.new(0, 0, 0.214285716, 0)
	TextLabel_2.Size = UDim2.new(0, 149, 0, 28)
	TextLabel_2.Font = Enum.Font.GothamSemibold
	TextLabel_2.Text = "Last Updated"
	TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.TextSize = 12.000

	TextLabel_3.Parent = Info
	TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_3.BackgroundTransparency = 1.000
	TextLabel_3.Position = UDim2.new(0, 0, 0.446428567, 0)
	TextLabel_3.Size = UDim2.new(0, 149, 0, 31)
	TextLabel_3.Font = Enum.Font.GothamSemibold
	TextLabel_3.Text = "4/26/2021"
	TextLabel_3.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_3.TextSize = 12.000

	UiCornerInfo.CornerRadius = UDim.new(0, 4)
	UiCornerInfo.Name = "UiCornerInfo"
	UiCornerInfo.Parent = Info

	CloseButtonInfo.Name = "CloseButtonInfo"
	CloseButtonInfo.Parent = Info
	CloseButtonInfo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CloseButtonInfo.BackgroundTransparency = 1.000
	CloseButtonInfo.Position = UDim2.new(0.898590326, 0, 0, 0)
	CloseButtonInfo.Size = UDim2.new(0, 15, 0, 18)
	CloseButtonInfo.ZIndex = 2
	CloseButtonInfo.Image = "http://www.roblox.com/asset/?id=6031094678"
	CloseButtonInfo.MouseButton1Click:Connect(function()
		Info.Visible = false
	end)

	Information.Name = "Information"
	Information.Parent = Frame
	Information.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Information.BackgroundTransparency = 1.000
	Information.Position = UDim2.new(0.774590552, 0, 0, 0)
	Information.Size = UDim2.new(0, 23, 0, 25)
	Information.ZIndex = 2
	Information.Image = "http://www.roblox.com/asset/?id=6026568227"
	Information.MouseButton1Click:Connect(function()
		Info.Visible = true
	end)

	local function YIQT_fake_script() -- MainFrame.LocalScript 
		local script = Instance.new('LocalScript', Frame)

		local UIS = game:GetService("UserInputService")
		function dragify(Frame)
			dragToggle = nil
			local dragSpeed = 0
			dragInput = nil
			dragStart = nil
			local dragPos = nil
			function updateInput(input)
				local Delta = input.Position - dragStart
				local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
				game:GetService("TweenService"):Create(Frame, TweenInfo.new(-10), {Position = Position}):Play()
			end
			Frame.InputBegan:Connect(function(input)
				if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
					dragToggle = true
					dragStart = input.Position
					startPos = Frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							dragToggle = false
						end
					end)
				end
			end)
			Frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					dragInput = input
				end
			end)
			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if input == dragInput and dragToggle then
					updateInput(input)
				end
			end)
		end

		dragify(script.Parent)
	end
	coroutine.wrap(YIQT_fake_script)()
	
	local nigga = {}
	function nigga:CreateButton(text, callback)
		callback = callback or function() end
		local TextButton = Instance.new("TextButton")
		local UICorner_2 = Instance.new("UICorner")




		TextButton.Parent = MainFrame
		TextButton.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
		TextButton.Position = UDim2.new(0.0673076957, 0, 0.119999997, 0)
		TextButton.Size = UDim2.new(0, 159, 0, 33)
		TextButton.Font = Enum.Font.GothamSemibold
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextButton.TextSize = 14.000
		TextButton.Text = text

		UICorner_2.CornerRadius = UDim.new(0, 3)
		UICorner_2.Parent = TextButton
		
		TextButton.MouseButton1Click:Connect(function()
			pcall(callback)
		end)

	end	
	return nigga
end
